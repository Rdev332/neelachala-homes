generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("NEXT_PUBLIC_DATABASE_URL")
}

model Contact {
  createdAt DateTime @default(now())
  email     String
  id        Int      @id @default(autoincrement())
  message   String
  name      String
  phone     String
}
